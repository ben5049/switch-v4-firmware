{
    "cmake.ignoreCMakeListsMissing": true,
    "flake8.args": [
        "--max-line-length=150",
        "--ignore=E262,E221,E501"
    ],
    "search.exclude": {
        "primary/Libraries/mstp/TestApp*": true,
        "primary/Libraries/zenoh-pico/examples/**": true,
        ".metadata": true,
    },
    "files.exclude": {
        "primary/Libraries/nanopb/.git*": true,
        "primary/Libraries/nanopb/build-tests": true,
        "primary/Libraries/nanopb/conan-wrapper": true,
        "primary/Libraries/nanopb/examples": true,
        "primary/Libraries/nanopb/extra": true,
        "primary/Libraries/nanopb/generator": true,
        "primary/Libraries/nanopb/spm*": true,
        "primary/Libraries/nanopb/tests": true,
        "primary/Libraries/nanopb/tools": true,
        "primary/Libraries/nanopb/zephyr": true,
        "primary/Libraries/nanopb/*bazel*": true,
        "primary/Libraries/nanopb/*.txt": true,
        "primary/Libraries/nanopb/*.py": true,
        "primary/Libraries/nanopb/*.md": true,
        "primary/Libraries/nanopb/*.json": true,
        "primary/Libraries/nanopb/*.swift": true,
        "primary/Libraries/nanopb/WORKSPACE": true,
        "primary/Libraries/mstp/TestApp*": true,
        "primary/Libraries/mstp/simulator": true,
        "primary/Libraries/mstp/*.*": true,
        "primary/Libraries/zenoh-pico/examples/**": true,
        "primary/Libraries/zenoh-pico/ci": true,
        "primary/Libraries/zenoh-pico/cmake": true,
        "primary/Libraries/zenoh-pico/tests": true,
        "primary/Libraries/zenoh-pico/tools": true,
        "primary/Libraries/zenoh-pico/zephyr": true,
        "primary/Libraries/zenoh-pico/*.*": true,
        "primary/Libraries/zenoh-pico/BSDmakefile": true,
        "primary/Libraries/zenoh-pico/GNUmakefile": true,
        ".metadata": true,
    },
    "search.useIgnoreFiles": true,
    "editor.defaultFormatter": "ms-vscode.cpptools",
}